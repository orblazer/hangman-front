{
  "name": "hangman-front",
  "version": "0.1.0",
  "description": "The front end of hangman",
  "author": "orblazer",
  "private": true,
  "scripts": {
    "develop": "gatsby clean && gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
